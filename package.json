{
  "name": "arco-vue3-supertable",
  "private": false,
  "version": "1.0.0",
  "description": "一个基于arco的vue3超级表格，基于配置来动态生成表格和表单",
  "keywords": [
    "vue",
    "vite",
    "arco"
  ],
  "author": "bokun.li",
  "license": "MIT",
  "main": "./dist/arco-vue3-supertable.umd.js",
  "module": "./dist/arco-vue3-supertable.es.js",
  "exports": {
    ".": {
      "import": "./dist/arco-vue3-supertable.es.js",
      "require": "./dist/arco-vue3-supertable.umd.js"
    },
    "./dist/arco-vue3-supertable.css": "./dist/arco-vue3-supertable.css"
  },
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist",
    "src/components"
  ],
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:types": "vue-tsc  --emitDeclarationOnly"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "vue": "^3.5.24",
    "vue-json-pretty": "^2.6.0"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.2.4",
    "vite-plugin-static-copy": "^3.1.4"
  }
}