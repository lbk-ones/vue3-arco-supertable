{
  "name": "arco-vue3-supertable",
  "private": false,
  "version": "1.0.2",
  "description": "一个基于arco的vue3超级表格，基于配置来动态生成表格和表单",
  "keywords": [
    "vue",
    "vite",
    "arco"
  ],
  "author": "bokun.li",
  "license": "MIT",
  "main": "./dist/arco-vue3-supertable.umd.js",
  "module": "./dist/arco-vue3-supertable.es.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/vue3-arco-supertable.d.ts",
      "import": "./dist/vue3-arco-supertable.es.js",
      "require": "./dist/vue3-arco-supertable.umd.js"
    },
    "./dist/arco-vue3-supertable.css": "./dist/arco-vue3-supertable.css"
  },
  "types": "./dist/types/vue3-arco-supertable.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "local-pack": "pnpm run build && pnpm pack",
    "type-check": "vue-tsc --noEmit --project tsconfig.types.json",
    "build:types": "vue-tsc --emitDeclarationOnly --project tsconfig.types.json"
  },
  "dependencies": {
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "axios": "^1.13.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4",
    "vue-json-pretty": "^2.6.0",
    "vue-tsc": "^3.2.2"
  }
}